{
  "name": "fotopia-serverless",
  "version": "1.0.1",
  "description": "Serverless photo web app using Lambda, S3, DynamoDB",
  "repository": {
    "type": "git",
    "url": "https://github.com/mbudm/fotopia-serverless.git"
  },
  "author": "Steve Roberts <mbudm.com@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "tape -r ts-node/register -r babel-register './fotos/**/*__test__.ts' | tap-spec",
    "test-watch": "tape-watch './fotos/**/*__test__.js' -r babel-register -p tap-spec",
    "functional-prod": "STAGE=prod tape ./test/functional.remote.js -r babel-register | tap-spec",
    "functional-dev": "STAGE=dev tape ./test/functional.remote.js -r babel-register | tap-spec",
    "functional-alpha": "STAGE=alpha tape -r ts-node/register -r babel-register ./test/functional.remote.ts | tap-spec",
    "functional-local": "IS_OFFLINE=1 tape ./test/functional.local.js -r babel-register | tap-spec",
    "lint": "tslint -c tslint.json 'fotos/**/*.ts'"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "aws4": "^1.8.0",
    "babel-runtime": "^6.26.0",
    "bunyan": "^1.8.12",
    "deep-get-set": "^1.1.0",
    "dotenv": "^8.0.0",
    "dynamodb-data-types": "^3.0.1",
    "leaked-handles": "^5.2.0",
    "serverless-domain-manager": "^3.2.6",
    "serverless-s3-remover": "^0.6.0",
    "serverless-stack-output": "^0.2.3",
    "sharp": "0.23.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.31",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.6.9",
    "@types/tape": "^4.2.33",
    "aws-amplify": "^1.1.35",
    "aws-sdk": "^2.503.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "dynamodb-local": "^0.0.29",
    "es6-promise": "^4.2.8",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "isomorphic-fetch": "^2.2.1",
    "pre-commit": "^1.2.2",
    "s3-upload-stream": "^1.0.7",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^5.8.0",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-s3-local": "^0.5.0",
    "serverless-s3-local-sync": "^1.0.8",
    "serverless-s3-sync": "^1.8.0",
    "serverless-webpack": "^5.3.1",
    "tap-dot": "^2.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0",
    "tape-watch": "^2.3.0",
    "ts-babel-node": "^1.1.1",
    "tslint": "^5.18.0",
    "webpack": "^4.39.1",
    "webpack-node-externals": "^1.7.2",
    "zlib": "^1.0.5"
  }
}
